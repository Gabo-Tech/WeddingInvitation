          <!-- Main -->
          <script src="/js/main.js"></script>
          <script>
            var d = new Date(new Date("August 4, 2023").getTime());
            var today = new Date();
            today.setHours(0, 0, 0, 0);
            
            if (d.getTime() === today.getTime()) {
            	document.querySelector(' .simply-countdown-one').innerHTML = '<h2>AVUI!</h2>';
            } else if (d < today) {
            	document.querySelector('.simply-countdown-one').innerHTML = "<h2>El 4 d'agost de 2023</h2>"
            		;
            } else {
            	simplyCountdown('.simply-countdown-one', {
            		year: d.getFullYear(), month:
            			d.getMonth() + 1, day: d.getDate(),
            	});
            }
            $('#simply-countdown-losange').simplyCountdown({
            	year: d.getFullYear(), month:
            		d.getMonth() + 1, day: d.getDate(), enableUtc: false,
            }); 
