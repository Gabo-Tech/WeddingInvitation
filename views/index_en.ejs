          <script>
            function playMusic() {
              let bgMusic = document.getElementById('bg-music');
              if (bgMusic.paused) { 
                bgMusic.loop = true;
                bgMusic.volume = 0.4;
                bgMusic.play();
              }
            }  
            window.addEventListener('load', playMusic);
            window.addEventListener('unload', playMusic);
            window.addEventListener('resize', playMusic);
            window.addEventListener('scroll', playMusic);
            window.addEventListener('keydown', playMusic);
            window.addEventListener('keyup', playMusic);
            window.addEventListener('keypress', playMusic);
            window.addEventListener('click', playMusic);
            window.addEventListener('dblclick', playMusic);
            window.addEventListener('mousedown', playMusic);
            window.addEventListener('mouseup', playMusic);
            window.addEventListener('mousemove', playMusic);
            window.addEventListener('mouseover', playMusic);
            window.addEventListener('mouseout', playMusic);
            window.addEventListener('mouseenter', playMusic);
            window.addEventListener('mouseleave', playMusic);
            window.addEventListener('submit', playMusic);
            window.addEventListener('change', playMusic);
            window.addEventListener('focus', playMusic);
            window.addEventListener('blur', playMusic);
            window.addEventListener('input', playMusic);
            window.addEventListener('touchstart', playMusic);
            window.addEventListener('touchend', playMusic);
            window.addEventListener('touchmove', playMusic);
            window.addEventListener('touchcancel', playMusic);
            window.addEventListener('dragstart', playMusic);
            window.addEventListener('drag', playMusic);
            window.addEventListener('dragenter', playMusic);
            window.addEventListener('dragleave', playMusic);
            window.addEventListener('dragover', playMusic);
            window.addEventListener('drop', playMusic);
            window.addEventListener('dragend', playMusic);
            document.getElementById('mapa').addEventListener('click', function() {
              const mapaIframe = document.getElementById('mapaIframe');
              if (mapaIframe.style.display === 'none' || mapaIframe.style.display === '') {
                mapaIframe.style.display = 'block';
              } else {
                mapaIframe.style.display = 'none';
              }
            });
            var d = new Date(new Date("August 4, 2023").getTime());
            var today = new Date();
            today.setHours(0, 0, 0, 0);
            if (d.getTime() === today.getTime()) {
            	document.querySelector(' .simply-countdown-one').innerHTML = '<h2>隆HOY!</h2>';
            } else if (d < today) {
            	document.querySelector('.simply-countdown-one').innerHTML = '<h2>El 4 de Agosto de 2023</h2>';
            } else {
            	simplyCountdown('.simply-countdown-one', {
            		year: d.getFullYear(), month:
            			d.getMonth() + 1, day: d.getDate(),
            	});
            }
            $('#simply-countdown-losange').simplyCountdown({
            	year: d.getFullYear(), month:
            		d.getMonth() + 1, day: d.getDate(), enableUtc: false,
            }); 
